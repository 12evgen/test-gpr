{
  "name": "pokemon-app",
  "description": "Pokemon frontend application",
  "version": "1.0.1",
  "author": "Popov Evgeniy <1212evgen@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/12evgen/pokemon-app.git"
  },
  "main": "src/index.js",
  "bin": {
    "pokemon-app": "./bin/server.prod.js"
  },
  "engines": {
    "node": "12.13.x",
    "npm": "6.12.x"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "rimraf _build_dev && node bin/server.dev.js",
    "prod": "node bin/build && cross-env PUBLIC_PATH=client/assets STATIC_PATH=server/static NODE_ENV=production node _build_prod",
    "build": "node bin/build",
    "format": "prettier --single-quote --semi=false --write \"{src,server,webpack}/**/*.js\" && eslint --fix src server webpack",
    "lint:css": "stylelint src/**/*.scss",
    "lint-css:fix": "stylelint src/**/*.scss --fix && prettier --write src/**/*.scss",
    "lint:js": "eslint src src/**/*.js",
    "lint-js:fix": "eslint src src/**/*.js --fix",
    "cm": "git-cz"
  },
  "dependencies": {
    "antd": "^3.26.6",
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "colors": "^1.3.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.5.4",
    "express": "^4.17.1",
    "express-no-favicons": "^0.0.1",
    "extract-css-chunks-webpack-plugin": "^4.7.1",
    "glob": "^7.1.4",
    "history": "^4.10.1",
    "http-proxy": "^1.16.2",
    "lodash": "^4.17.15",
    "mkdirp": "^1.0.3",
    "mobx": "^5.15.0",
    "mobx-react": "^6.1.4",
    "mobx-react-devtools": "^6.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-universal-component": "^4.0.0",
    "serialize-javascript": "^2.1.0",
    "serviceworker-webpack-plugin": "^1.0.1",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-flush-chunks": "^2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-do-expressions": "^7.5.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.3.2",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
